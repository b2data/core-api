{
  "name": "@b2data/core-api",
  "description": "Core API interfaces",
  "version": "1.0.6",
  "scripts": {
    "build": "tsc",
    "gen": "npm run gen:auth && npm run gen:providers && npm run gen:apps && npm run gen:dictionary",
    "gen:auth": "swagger-typescript-api -p auth-openapi.yaml ${npm_package_config_gen} -n AuthApi.ts",
    "gen:providers": "swagger-typescript-api -p providers-openapi.yaml ${npm_package_config_gen} -n ProvidersApi.ts",
    "gen:apps": "swagger-typescript-api -p apps-openapi.yaml ${npm_package_config_gen} -n AppsApi.ts",
    "gen:dictionary": "swagger-typescript-api -p dictionary-openapi.yaml ${npm_package_config_gen} -n DictionaryApi.ts"
  },
  "config": {
    "gen": "--unwrap-response-data --module-name-first-tag --single-http-client --union-enums"
  },
  "license": "MIT",
  "keywords": [],
  "author": "Nikolai Turov",
  "files": [
    "HttpClient*",
    "*Api.ts",
    "*Api.js",
    "*.yaml"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "devDependencies": {
    "swagger-typescript-api": "^13.0.3"
  }
}
